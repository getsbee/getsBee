{
  "manifest_version": 3,
  "name": "getsBee",
  "description": "highlight sentences and share",
  "version": "1.9",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "/utils/constant.js",
        "/utils/validation.js",
        "/views/View.js",
        "/views/TooltipView.js",
        "/views/ColorBtnView.js",
        "/views/TrashBtnView.js",
        "/views/HighlightView.js",
        "/views/CloseBtnView.js",
        "/model/pageModel.js",
        "/model/selectionModel.js",
        "/model/loginModel.js",
        "/model/highlightModel.js",
        "/model/LocalStorageModel.js",
        "/model/recommendModel.js",
        "/controller/MainController.js",
        "/dto/highlightDTO.js",
        "/utils/api.js",
        "app.js"
        
      ],
      "css": ["getsBeeCustom.css"],
      "type": "module",
      "host_permissions": [
        "https://velog.io/*",
        "https://www.withme.my/*"
      ]
    }
  ]
}
